  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="TamagotchiDomain.TamagotchiRikSimonEntities" />
    <_ConnectionStringsToInsert Include="TamagotchiService.Data.TamagotchiContext" />
  </ItemGroup>
