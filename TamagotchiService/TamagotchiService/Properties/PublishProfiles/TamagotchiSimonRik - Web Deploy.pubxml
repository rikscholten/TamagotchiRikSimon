            <Source Path="DBContext" DbContext="TamagotchiService.Data.TamagotchiContext, TamagotchiService" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="TamagotchiDomain.TamagotchiRikSimonEntities" Order="2" Enabled="False">
          <Destination Path="" />
